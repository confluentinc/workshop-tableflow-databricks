{
    "name": "customer_generator_streaming",
    "table": "cdc.customer",
    "connection": "postgres",
    "localConfigs": {
        "maxEvents": 240,
        "throttleMs": 30000
    },
    "row": {
        "customer_id": {
            "_gen": "sequentialString",
            "startingFrom": 40000001,
            "expr": "C~d",
            "pgHint": "VARCHAR(50)"
        },
        "email": {
            "_gen": "string",
            "expr": "#{Internet.emailAddress}",
            "pgHint": "VARCHAR(255) PRIMARY KEY"
        },
        "first_name": {
            "_gen": "string",
            "expr": "#{Name.firstName}",
            "pgHint": "VARCHAR(100)"
        },
        "last_name": {
            "_gen": "string",
            "expr": "#{Name.lastName}",
            "pgHint": "VARCHAR(100)"
        },
        "birth_date": {
            "_gen": "string",
            "expr": "#{Date.birthday 'yyyy-MM-dd'}",
            "pgHint": "VARCHAR(10)"
        },
        "created_at": {
            "_gen": "now",
            "serialize": {
                "type": "postgresTimestamp"
            },
            "pgHint": "TIMESTAMPTZ"
        },
        "updated_at": {
            "_gen": "now",
            "serialize": {
                "type": "postgresTimestamp"
            },
            "pgHint": "TIMESTAMPTZ"
        }
    }
}
